{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
	{
		"type": "npm",
		"script": "build",
		"problemMatcher": [
			"$eslint-stylish"
		]
	},
	{
		"type": "npm",
		"script": "build:watch",
		"problemMatcher": [
			"$eslint-stylish"
		]
	},
	{
		"type": "npm",
		"script": "start",
		"problemMatcher": [
			"$eslint-stylish"
		]
	},
	{
		"type": "npm",
		"script": "build",
		"path": "frontend/",
		"problemMatcher": [
			"$eslint-stylish"
		]
	},
	{
		"type": "npm",
		"script": "build:watch",
		"path": "frontend/",
		"problemMatcher": [
			"$eslint-stylish"
		]
	},
	{
		"type": "npm",
		"script": "test:cov",
		"problemMatcher": [
			"$eslint-stylish"
		]
	},
	{
		"type": "npm",
		"script": "test:e2e",
		"problemMatcher": [
			"$eslint-stylish"
		]
	},
	{
		"type": "npm",
		"script": "test:e2e",
		"path": "frontend/",
		"problemMatcher": [
			"$eslint-stylish"
		]
	},
	{
		"type": "npm",
		"script": "test:coverage",
		"path": "frontend/",
		"problemMatcher": [
			"$eslint-stylish"
		]
	},
	{
		"type": "npm",
		"script": "test:watch",
		"path": "frontend/",
		"problemMatcher": [
			"$eslint-stylish"
		]
	},
	{
		"type": "npm",
		"script": "test:watch",
		"problemMatcher": [
			"$eslint-stylish"
		]
	},
	{
		"type": "npm",
		"script": "lint",
		"problemMatcher": [
			"$eslint-stylish"
		]
	},
	{
		"type": "npm",
		"script": "start:watch:dev",
		"problemMatcher": [
			"$eslint-stylish"
		]
	},
	{
		"type": "npm",
		"script": "start:watch:debug",
		"problemMatcher": [
			"$eslint-stylish"
		]
	},
	{
		"type": "npm",
		"script": "start",
		"path": "frontend/",
		"problemMatcher": [
			"$eslint-stylish"
		]
	},
	{
		"type": "npm",
		"script": "lint",
		"path": "frontend/",
		"problemMatcher": [
			"$eslint-stylish"
		]
	},
	{
		"type": "gulp",
		"task": "default",
		"problemMatcher": [
			"$eslint-stylish"
		]
	},
	{
		"label": "docker-compose build",
		"type": "shell",
		"command": "docker-compose",
		"args": [
			"build"
		]
	},
	{
		"type": "npm",
		"script": "start:prod",
		"problemMatcher": [
			"$eslint-stylish"
		]
	},
	{
		"type": "npm",
		"script": "test:e2e:watch",
		"problemMatcher": [
			"$awesomets-lint"
		],
		"group": "build"
	}
]
}
