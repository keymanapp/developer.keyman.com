{
  "name": "keyman-developer-online",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc --watch -p tsconfig.build.json",
    "gulp": "gulp",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "cross-env NODE_ENV=development  ts-node -r tsconfig-paths/register src/main.ts",
    "start:watch:dev": "cross-env NODE_ENV=development  nodemon",
    "start:watch:debug": "cross-env NODE_ENV=development  nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "cross-env NODE_ENV=production  node dist/main.js",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "test": "cross-env NODE_ENV=test jest --verbose",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test  jest --coverage",
    "test:debug": "cross-env NODE_ENV=test  node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test  jest --verbose --config ./test/jest-e2e.json",
    "test:e2e:watch": "cross-env NODE_ENV=test  jest --verbose --config ./test/jest-e2e.json --watch --onlyChanged",
    "test:e2e:debug": "cross-env NODE_ENV=test  node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nest-middlewares/cookie-parser": "^6.0.0",
    "@nest-middlewares/cookie-session": "^6.0.0",
    "@nest-middlewares/express-session": "^6.0.1",
    "@nest-middlewares/helmet": "^6.0.0",
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/swagger": "^4.8.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.2.6",
    "express-session": "^1.17.1",
    "helmet": "^4.5.0",
    "memorystore": "^1.6.6",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.7",
    "simple-git": "^2.38.0",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.6.15",
    "@schematics/angular": "^11.2.9",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/express-rate-limit": "^5.1.1",
    "@types/express-serve-static-core": "^4.17.19",
    "@types/express-session": "^1.17.3",
    "@types/hapi__joi": "^17.1.6",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "axios-mock-adapter": "^1.19.0",
    "base-64": "^1.0.0",
    "colors": "^1.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "gulp": "^4.0.2",
    "gulp-run-command": "0.0.10",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "supertest-session": "^4.1.0",
    "ts-jest": "^26.5.5",
    "ts-mockito": "^2.6.1",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "jest": {
    "projects": [
      {
        "rootDir": "src",
        "moduleFileExtensions": [
          "js",
          "json",
          "ts"
        ],
        "testRegex": ".spec.ts$",
        "transform": {
          "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "<rootDir>/../coverage",
        "testEnvironment": "node"
      }
    ]
  }
}
