{
  "name": "keyman-developer-online",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc --watch -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "cross-env NODE_ENV=development  ts-node -r tsconfig-paths/register src/main.ts",
    "start:watch:dev": "cross-env NODE_ENV=development  nodemon",
    "start:watch:debug": "cross-env NODE_ENV=development  nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "cross-env NODE_ENV=production  node dist/main.js",
    "lint": "tslint -p tsconfig.lint.json -c tslint.json",
    "test": "cross-env NODE_ENV=test  jest",
    "test:watch": "cross-env NODE_ENV=test  jest --watch",
    "test:cov": "cross-env NODE_ENV=test  jest --coverage",
    "test:debug": "cross-env NODE_ENV=test  node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test  jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@hapi/joi": "^15.0.3",
    "@nest-middlewares/cookie-parser": "^6.0.0",
    "@nest-middlewares/cookie-session": "^6.0.0",
    "@nest-middlewares/express-session": "^6.0.0",
    "@nest-middlewares/helmet": "^6.0.0",
    "@nestjs/common": "^6.3.0",
    "@nestjs/core": "^6.3.0",
    "@nestjs/platform-express": "^6.3.0",
    "@types/helmet": "0.0.43",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "csurf": "^1.10.0",
    "dotenv": "^8.0.0",
    "express-rate-limit": "<3.5.2",
    "helmet": "^3.18.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.5.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.3.0",
    "@schematics/angular": "^8.0.1",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/express-rate-limit": "^3.3.0",
    "@types/express-session": "^1.15.12",
    "@types/hapi__joi": "^15.0.1",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.4",
    "@types/supertest": "^2.0.7",
    "axios-mock-adapter": "^1.16.0",
    "cross-env": "^5.2.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.17.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-mockito": "^2.3.1",
    "ts-node": "^8.2.0",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1"
  },
  "jest": {
    "projects": [
      {
        "rootDir": "src",
        "moduleFileExtensions": [
          "js",
          "json",
          "ts"
        ],
        "testRegex": ".spec.ts$",
        "transform": {
          "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "<rootDir>/../coverage",
        "testEnvironment": "node"
      },
      {
        "rootDir": "frontend",
        "preset": "jest-preset-angular",
        "setupFilesAfterEnv": [
          "<rootDir>/src/setupJest.ts"
        ],
        "runner": "jest-runner-tsc"
      }
    ]
  }
}
